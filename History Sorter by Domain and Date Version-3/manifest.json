{
  "manifest_version": 3,
  "name": "History Sorter by Domain and Date",
  "version": "1.0",
  "description": "Organizes browsing history by domain and sorts by date.",
  "permissions": ["history", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
